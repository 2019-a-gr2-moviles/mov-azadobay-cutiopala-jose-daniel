<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Lectores.MenuLectores" android:background="@drawable/fondohome">

    <Button
            android:text="@string/listaLectores"
            android:layout_width="195dp"
            android:layout_height="65dp"
            android:id="@+id/btn_listaLectores" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.09"
            android:layout_marginTop="260dp" app:layout_constraintTop_toTopOf="parent"
            android:textStyle="bold" android:textAllCaps="true" android:textAlignment="center" android:textSize="18sp"/>
    <Button
            android:text="@string/nuevoLector"
            android:layout_width="191dp"
            android:layout_height="62dp"
            android:id="@+id/btn_nuevoLector"
            app:layout_constraintEnd_toEndOf="@+id/btn_ver_cli"
            android:textStyle="bold" android:textAllCaps="true" android:textAlignment="center" android:textSize="18sp"
            app:layout_constraintStart_toStartOf="@+id/btn_listaLectores"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginTop="48dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_listaLectores"/>

</androidx.constraintlayout.widget.ConstraintLayout>