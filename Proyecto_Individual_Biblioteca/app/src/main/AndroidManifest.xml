<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.proyecto_biblioteca">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true">
        <activity android:name=".Lectores.ListaLector">
        </activity>
        <activity android:name=".Lectores.ActualizarLector">
        </activity>
        <activity android:name=".Lectores.CrearLector">
        </activity>
        <activity android:name=".Lectores.ListaLectores">
        </activity>
        <activity android:name=".MenuSolicitudes">
        </activity>
        <activity android:name=".Lectores.MenuLectores">
        </activity>
        <activity android:name=".Libros.ListaLibros">
        </activity>
        <activity android:name=".Libros.CrearLibro">
        </activity>
        <activity android:name=".Libros.Actualizar_Libro">
        </activity>
        <activity android:name=".Libros.MenuLibros">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>